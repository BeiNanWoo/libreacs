    <lifecycle>
        <phase-listener>org.openacs.web.PhaseListener</phase-listener>
    </lifecycle>
    <managed-bean>
        <managed-bean-name>skinBean</managed-bean-name>
        <managed-bean-class>org.openacs.web.SkinBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>skin</property-name>
            <property-class>java.lang.String</property-class>
            <value>openacs</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jsfHostsBean</managed-bean-name>
        <managed-bean-class>org.openacs.web.HostsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FormsBean</managed-bean-name>
        <managed-bean-class>org.openacs.web.Form</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>SoftwaresBean</managed-bean-name>
        <managed-bean-class>org.openacs.web.SoftwaresBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ConfigsBean</managed-bean-name>
        <managed-bean-class>org.openacs.web.ConfigsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>CpeBean</managed-bean-name>
        <managed-bean-class>org.openacs.web.CpeBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ScriptsBean</managed-bean-name>
        <managed-bean-class>org.openacs.web.ScriptsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>HardwareModelsBean</managed-bean-name>
        <managed-bean-class>org.openacs.web.HardwareModelsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Script</managed-bean-name>
        <managed-bean-class>org.openacs.web.ScriptJsfBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>HwModel</managed-bean-name>
        <managed-bean-class>org.openacs.web.HwModelJsfBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Config</managed-bean-name>
        <managed-bean-class>org.openacs.web.ConfigJsfBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Software</managed-bean-name>
        <managed-bean-class>org.openacs.web.SoftwareJsfBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Cpe</managed-bean-name>
        <managed-bean-class>org.openacs.web.HostsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <navigation-case>
            <from-outcome>t2</from-outcome>
            <to-view-id>/t2.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>t1</from-outcome>
            <to-view-id>/t1.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/cpe_find.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>notfound</from-outcome>
            <to-view-id>/cpe_find.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/cpe_find.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>foundone</from-outcome>
            <to-view-id>/cpe_overview.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/cpe_find.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>foundmany</from-outcome>
            <to-view-id>/cpe_foundmany.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/cpe_find.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>config</from-outcome>
            <to-view-id>/cpe_config.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/deviceprofile_new.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>created</from-outcome>
            <to-view-id>/deviceprofile_edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/deviceprofile_edit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>deleted</from-outcome>
            <to-view-id>/deviceprofile_new.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/deviceprofile_edit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>create</from-outcome>
            <to-view-id>/deviceprofile_new.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/deviceprofile_new.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>loaded</from-outcome>
            <to-view-id>/deviceprofile_edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>created_service</from-outcome>
            <to-view-id>/service_edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>created_voiceprofile</from-outcome>
            <to-view-id>/service_edit_voiceprofile.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>created_phoneline</from-outcome>
            <to-view-id>/service_edit_phoneline.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>deleted_service</from-outcome>
            <to-view-id>/service_new.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>create_service</from-outcome>
            <to-view-id>/service_new.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>loaded_voiceprofile</from-outcome>
            <to-view-id>/service_edit_voiceprofile.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>loaded_phoneline</from-outcome>
            <to-view-id>/service_edit_phoneline.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>loaded_service</from-outcome>
            <to-view-id>/service_edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <managed-bean>
        <managed-bean-name>DSLStats</managed-bean-name>
        <managed-bean-class>org.openacs.web.DSLStatsJsfBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>DeviceProfile</managed-bean-name>
        <managed-bean-class>org.openacs.web.DeviceProfileBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Service</managed-bean-name>
        <managed-bean-class>org.openacs.web.ServiceBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>LineStatsGraphData</managed-bean-name>
        <managed-bean-class>org.openacs.web.LineStatsGraphData</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Settings</managed-bean-name>
        <managed-bean-class>org.openacs.web.SettingsBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/devices_new.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>hwloaded</from-outcome>
            <to-view-id>/devices_edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>hwdeleted</from-outcome>
            <to-view-id>/devices_new.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>hwcleared</from-outcome>
            <to-view-id>/devices_new.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>swcleared</from-outcome>
            <to-view-id>/software_new.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>swloaded</from-outcome>
            <to-view-id>/software_edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>swdeleted</from-outcome>
            <to-view-id>/software_new.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>swcreated</from-outcome>
            <to-view-id>/software_edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>cfgloaded</from-outcome>
            <to-view-id>/config_edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>cfgcleared</from-outcome>
            <to-view-id>/config_new.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>cfgcreated</from-outcome>
            <to-view-id>/config_edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>cfgdeleted</from-outcome>
            <to-view-id>/config_new.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>swmethods</from-outcome>
            <to-view-id>/software_methods.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>cperemoved</from-outcome>
            <to-view-id>/cpe_find.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>swparamnames</from-outcome>
            <to-view-id>/software_paramnames.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>hwcreated</from-outcome>
            <to-view-id>/devices_edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/cpe_foundmany.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cpegetitem</from-outcome>
            <to-view-id>/cpe_overview.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>DataModel</managed-bean-name>
        <managed-bean-class>org.openacs.web.DataModelJsfBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <component>
        <component-type>org.openacs.Param</component-type>
        <component-class>org.openacs.web.tags.Param</component-class>
    </component>
    <component>
        <component-type>org.openacs.Form</component-type>
        <component-class>org.openacs.web.tags.Form</component-class>
    </component>
    <managed-bean>
        <managed-bean-name>testbean</managed-bean-name>
        <managed-bean-class>org.openacs.web.testbean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>
