<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.j2ee.ejbjarproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/j2ee-ejbjarproject/3">
            <name>acs-ejb</name>
            <minimum-ant-version>1.6</minimum-ant-version>
            <included-library>file.reference.js.jar</included-library>
            <included-library>file.reference.commons-httpclient-3.1.jar</included-library>
            <included-library>file.reference.jboss-jaxrpc.jar</included-library>
            <web-services>
                <web-service>
                    <web-service-name>OpenACS</web-service-name>
                </web-service>
            </web-services>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
    </configuration>
</project>
